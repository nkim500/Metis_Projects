   "outputs": [],
   "source": [
    "mask = ((df4_n[\"H\"] == 9) | (df4_n[\"H\"] == 10) | (df4_n[\"H\"] == 11) |\\\n",
    "        (df4_n[\"H\"] == 13) | (df4_n[\"H\"] == 14) | (df4_n[\"H\"] == 15) |\\\n",
    "        (df4_n[\"H\"] == 16) | (df4_n[\"H\"] == 17) | (df4_n[\"H\"] == 17))\n",
    "disp_H = df4_n[mask]"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 39,
   "id": "ba533b82-9213-4ac8-b120-2ba25cf9b728",
